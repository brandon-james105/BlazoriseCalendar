{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.3.0-alpha1",
      "destination": "wwwroot/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.min.css"
      ]
    },
    {
      "library": "bootstrap-icons@1.10.3",
      "destination": "wwwroot/bootstrap-icons/",
      "files": [
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.min.css"
      ]
    }
  ]
}